    <dataTable>
      <entityName>reml example</entityName>
      <entityDescription>An example, intended for
                              illustrative purposes only.</entityDescription>
      <physical>
        <objectName>reml_example.csv</objectName>
        <size>2464</size>
        <characterEncoding>en_US.UTF-8</characterEncoding>
        <dataFormat>
          <textFormat>
            <numHeaderLines>1</numHeaderLines>
            <numFooterLines>0</numFooterLines>
            <recordDelimiter>\r\n</recordDelimiter>
            <attributeOrientation>column</attributeOrientation>
            <simpleDelimited>
              <fieldDelimiter>,</fieldDelimiter>
            </simpleDelimited>
          </textFormat>
        </dataFormat>
      </physical>
      <attributeList>
        <attribute>
          <attributeName>river</attributeName>
          <attributeDefinition>River site used for collection</attributeDefinition>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>SAC</code>
                    <definition>The Sacramento River</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>AM</code>
                    <definition>The American River</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>spp</attributeName>
          <attributeDefinition>Species common name</attributeDefinition>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>king</code>
                    <definition>King Salmon</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>ccho</code>
                    <definition>Coho Salmon</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>stg</attributeName>
          <attributeDefinition>Life Stage</attributeDefinition>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>parr</code>
                    <definition>third life stage</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>smolt</code>
                    <definition>fourth life stage</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>ct</attributeName>
          <attributeDefinition>count</attributeDefinition>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <textDomain>
                  <definition>number</definition>
                </textDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
      </attributeList>
      <caseSensitive>yes</caseSensitive>
      <numberOfRecords>3</numberOfRecords>
